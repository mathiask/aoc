#+begin_src elisp
(goto-char 1)
(goto-char (point-max)

(search-forward "some string")
(re-search-forward "^;;;;; INPUT$")
(re-search-forward "^..." (line-end-position))
(not (looking-at "^$"))

(while (not (or (eobp) (looking-at "^;;;;;")))

substring-no-properties
number-sequence, dolist, dotimes, mapc, mapchar
seq-filter, seq-map, seq-do-indexed, seq-map-indexed

(split-string s ",")

eq, =, string=
